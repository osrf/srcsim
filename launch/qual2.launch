<launch>
  <arg name="paused" value="false" />
  <arg name="verbose" value="true" />
  <arg name="world_name" value="$(find srcsim)/worlds/qual2.world" />
  <arg name="extra_gazebo_args" default=""/>

  <include file="$(find val_gazebo)/launch/val_sim_gazebo_sync.launch" pass_all_args="true">
    <arg name="paused" value="$(arg paused)" />
    <arg name="verbose" value="$(arg verbose)" />
    <arg name="world_name" value="$(arg world_name)" />
    <arg name="extra_gazebo_args" default="$(arg extra_gazebo_args)"/>
  </include>

  <include file="$(find ihmc_valkyrie_ros)/launch/valkyrie_ros_api.launch" pass_all_args="true">
    <arg name="use_local_build" value="true" />
  </include>

  <include file="$(find ihmc_valkyrie_ros)/launch/val_wholebody_control_gazebo.launch"/>

  <group if="$(arg init)">
    <node name="startup_robot" pkg="srcsim" type="init_robot.sh" args="" output="screen"/>
  </group>

  <!--
  <include file="$(find ihmc_valkyrie_ros)/launch/remote_valkyrie_visualizer.launch" pass_all_args="true">
    <arg name="use_local_build" value="true" />
  </include>
  -->
</launch>
